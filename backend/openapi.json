          "address": {
            "$ref": "#/components/schemas/CompanyAddress"
          },
          "gstins": {
            "items": {
              "$ref": "#/components/schemas/CompanyGSTINCreate"
            },
            "type": "array",
            "title": "Gstins",
            "default": []
