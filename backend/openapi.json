        },
        "type": "object",
        "required": [
          "legal_name",
          "contacts",
          "address"
        ],
        "title": "CompanyCreate"
      },
      "CompanyGSTINCreate": {
        "properties": {
          "gstin": {
            "type": "string",
            "pattern": "^\\d{2}[A-Z]{5}\\d{4}[A-Z]{1}[A-Z\\d]{1}[Z]{1}[A-Z\\d]{1}$",
            "title": "Gstin"
          },
          "is_primary": {
            "type": "boolean",
            "title": "Is Primary",
            "default": false
          }
        },
        "type": "object",
        "required": [
          "gstin"
        ],
        "title": "CompanyGSTINCreate"
