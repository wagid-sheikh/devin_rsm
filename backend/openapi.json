  "components": {
    "schemas": {
      "CompanyAddress": {
        "properties": {
          "address_line1": {
            "type": "string",
            "maxLength": 200,
            "minLength": 1,
            "title": "Address Line1"
          },
          "address_line2": {
            "anyOf": [
              {
                "type": "string",
                "maxLength": 200
              },
              {
                "type": "null"
              }
            ],
            "title": "Address Line2"
          },
          "city": {
            "type": "string",
            "maxLength": 100,
            "minLength": 1,
            "title": "City"
          },
          "state": {
            "type": "string",
            "maxLength": 100,
            "minLength": 1,
            "title": "State",
            "description": "Indian state name"
          },
          "pincode": {
            "type": "string",
            "pattern": "^\\d{6}$",
            "title": "Pincode",
            "description": "6-digit Indian postal code"
          },
          "country": {
            "type": "string",
            "maxLength": 100,
            "title": "Country",
            "default": "India"
          },
          "landmark": {
            "anyOf": [
              {
                "type": "string",
                "maxLength": 200
              },
              {
                "type": "null"
              }
            ],
            "title": "Landmark"
          }
        },
        "type": "object",
        "required": [
          "address_line1",
          "city",
          "state",
          "pincode"
        ],
        "title": "CompanyAddress"
      },
      "CompanyContacts": {
        "properties": {
          "email": {
            "type": "string",
            "format": "email",
            "title": "Email"
          },
          "phone": {
            "type": "string",
            "pattern": "^\\+?[1-9]\\d{1,14}$",
            "title": "Phone",
            "description": "Phone number in E.164 format"
          },
          "alternate_phone": {
            "anyOf": [
              {
                "type": "string",
                "pattern": "^\\+?[1-9]\\d{1,14}$"
              },
              {
                "type": "null"
              }
            ],
            "title": "Alternate Phone",
            "description": "Alternate phone in E.164 format"
          },
          "website": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Website"
          },
          "contact_person_name": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Contact Person Name"
          },
          "contact_person_designation": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Contact Person Designation"
          }
        },
        "type": "object",
        "required": [
          "email",
          "phone"
        ],
        "title": "CompanyContacts"
      },
      "CompanyCreate": {
        "properties": {
          "legal_name": {
            "type": "string",
            "title": "Legal Name"
          },
          "trade_name": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Trade Name"
          },
          "pan": {
            "anyOf": [
